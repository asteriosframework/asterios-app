{
  "name": "asterios/app",
  "type": "project",
  "description": "Scaffold App for AsteriosPHP. AsteriosPHP is a simple and flexible PHP 8.+ framework, inspired from the best features of other frameworks, in a modern and elegant way.",
  "homepage": "https://www.asteriosphp.de",
  "authors": [
    {
      "name": "Benjamin Butschko"
    },
    {
      "name": "JÃ¶rg Heinrich"
    }
  ],
  "require": {
    "php": ">=8.3",
    "asterios/core": "^1.5",
    "ext-intl": "*",
    "ext-ctype": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "asterios-install": [
      "@php vendor/bin/asterios install"
    ],
    "post-install-cmd": [
      "@asterios-install"
    ],
    "post-update-cmd": [
      "@asterios-install"
    ]
  }

}