{
  "name": "asterios/app",
  "type": "project",
  "require": {
    "php": ">=8.3",
    "asterios/core": "dev-develop",
    "ext-intl": "*",
    "ext-ctype": "*"
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:asteriosframework/core.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true
}